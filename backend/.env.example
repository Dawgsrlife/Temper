# ──────────────────────────────────────────
# Temper Backend – Environment Variables
# Copy to .env and fill in values
# ──────────────────────────────────────────

# Debug mode
DEBUG=true

# Database (Postgres via Supabase / Neon / local)
DATABASE_URL=postgresql://user:password@localhost:5432/temper?schema=public

# CORS Origins (comma-separated)
CORS_ORIGINS=http://localhost:3000

# Optional: OpenAI for coach layer
# OPENAI_API_KEY=sk-...
