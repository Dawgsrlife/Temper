# ──────────────────────────────────────────────────────────────
# Temper – Environment Variables
# Copy to .env and fill in your values
# ──────────────────────────────────────────────────────────────

# ─── Supabase ─────────────────────────────────────────────────
SUPABASE_URL=https://your-project.supabase.co
SUPABASE_ANON_KEY=your-anon-key
SUPABASE_SERVICE_ROLE_KEY=your-service-role-key
DATABASE_URL=postgresql://postgres:[password]@db.[project-ref].supabase.co:5432/postgres

# ─── Frontend (Next.js) ───────────────────────────────────────
NEXT_PUBLIC_API_URL=http://localhost:8000
NEXT_PUBLIC_SUPABASE_URL=${SUPABASE_URL}
NEXT_PUBLIC_SUPABASE_ANON_KEY=${SUPABASE_ANON_KEY}

# ─── Backend (FastAPI) ────────────────────────────────────────
BACKEND_PORT=8000
CORS_ORIGINS=http://localhost:3000

# ─── AI / LLM ─────────────────────────────────────────────────
OPENAI_API_KEY=sk-...
OPENROUTER_API_KEY=
OPENROUTER_MODEL=google/gemini-2.0-flash-001
OPENROUTER_HTTP_REFERER=
OPENROUTER_APP_TITLE=Temper
GEMINI_API_KEY=
GOOGLE_API_KEY=
VERTEX_PROJECT_ID=
VERTEX_LOCATION=us-central1
VERTEX_MODEL=gemini-1.5-flash-002
# Optional direct bearer token. If unset, backend uses ADC (GOOGLE_APPLICATION_CREDENTIALS).
VERTEX_ACCESS_TOKEN=
GOOGLE_APPLICATION_CREDENTIALS=
GOOGLE_CLOUD_PROJECT=

# ─── Voice (Coach Narration) ──────────────────────────────────
ELEVENLABS_API_KEY=
ELEVENLABS_SPEECH_ID=
# Optional overrides
ELEVENLABS_MODEL_ID=eleven_multilingual_v2
ELEVENLABS_OUTPUT_FORMAT=mp3_44100_128

# ─── Gradium (TTS fallback + STT journaling) ──────────────────
GRADIUM_API_KEY=
GRADIUM_SPEECH_ID=
# Optional endpoint overrides (defaults are built into backend/app/voice.py)
GRADIUM_TTS_URL=https://api.gradium.ai/api/post/speech/speak
GRADIUM_STT_URL=https://api.gradium.ai/api/post/speech/transcribe

# ─── Debug ────────────────────────────────────────────────────
DEBUG=true
UPLOADTHING_TOKEN=
MAX_UPLOAD_MB=250
